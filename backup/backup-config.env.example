# Backup Configuration for ps.kz S3 Storage
# Copy this file to backup-config.env and fill in your credentials

# PostgreSQL Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=engcrm
DB_USER=engcrm_user
DB_PASSWORD=your_db_password_here

# ps.kz S3 Storage
S3_ENDPOINT_URL=https://s3.ps.kz
S3_ACCESS_KEY_ID=your_access_key_here
S3_SECRET_ACCESS_KEY=your_secret_key_here
S3_BUCKET_NAME=engcrm-backups
S3_REGION=kz-astana-1

# Backup Settings
BACKUP_RETENTION_DAYS=7
BACKUP_PREFIX=postgres-backup
